<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.denguecheckerapp">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DengueCheckerApp"
        tools:targetApi="31">

        <activity
            android:name="com.example.dengueapp.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.DengueCheckerApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.dengueapp.ResultadosActivity"
            android:exported="true"
            android:theme="@style/AppTheme">
        </activity>

        <activity android:name="com.example.dengueapp.AnaliseActivity"
            android:exported="true"
            android:theme="@style/AppTheme">
        </activity>

        <activity android:name="com.example.dengueapp.ListaPacientesActivity"
            android:exported="true"
            android:theme="@style/AppTheme">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>